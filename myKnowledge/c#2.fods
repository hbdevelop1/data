     <table:table-cell/>
     <table:table-cell table:style-name="ce73" office:value-type="string" calcext:value-type="string">
      <text:p><text:s text:c="12"/>Tenon CodeBuilder supports Objective-C. <text:s/>This product is based</text:p>
