     </table:table-row>
     <table:table-row table:style-name="ro1" table:visibility="collapse">
      <table:table-cell/>
      <table:table-cell office:value-type="string" calcext:value-type="string">
       <text:p><text:s text:c="4"/>{ <text:s text:c="7"/></text:p>
