    </table:table-row>
    <table:table-row table:style-name="ro19">
     <table:table-cell/>
     <table:table-cell table:style-name="ce73" office:value-type="string" calcext:value-type="string">
      <text:p><text:s text:c="4"/>class posing.</text:p>
