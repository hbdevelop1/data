    <table:table-row table:style-name="ro20">
     <table:table-cell/>
     <table:table-cell table:style-name="ce64" office:value-type="string" calcext:value-type="string">
      <text:p>prevent a member function from being overriden</text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row-group table:display="false">
     <table:table-row table:style-name="ro20" table:visibility="collapse">
      <table:table-cell/>
      <table:table-cell office:value-type="string" calcext:value-type="string">
       <text:p>solution1: set as non-virutal</text:p>
      </table:table-cell>
     </table:table-row>
     <table:table-row table:style-name="ro20" table:visibility="collapse">
      <table:table-cell/>
      <table:table-cell office:value-type="string" calcext:value-type="string">
       <text:p>solution2: use final</text:p>
      </table:table-cell>
     </table:table-row>
     <table:table-row table:style-name="ro20" table:visibility="collapse">
      <table:table-cell/>
      <table:table-cell office:value-type="string" calcext:value-type="string">
       <text:p>solution3: use a comment to discourage from overriding</text:p>
      </table:table-cell>
     </table:table-row>
     <table:table-row table:style-name="ro20" table:visibility="collapse">
      <table:table-cell table:number-columns-repeated="2"/>
     </table:table-row>
    </table:table-row-group>
    <table:table-row table:style-name="ro20">
