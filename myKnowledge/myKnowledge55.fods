    <table:table-row table:style-name="ro1">
     <table:table-cell/>
     <table:table-cell table:style-name="ce4" office:value-type="string" calcext:value-type="string">
      <text:p>C++11 introduced { } initialization</text:p>
     </table:table-cell>
